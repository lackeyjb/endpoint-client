{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "array-includes",
    "syntax-class-properties",
    "transform-class-properties",
    "syntax-object-rest-spread",
    "transform-object-rest-spread",
    "transform-object-assign"
  ],
  "env": {
    "watch": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
